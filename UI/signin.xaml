<Window x:Class="UI.signin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI"
        mc:Ignorable="d"
        Title="signin" Height="318.944" Width="318.012">
    <Grid>

        <TextBox Name="txtBox_login" HorizontalAlignment="Left" Height="33" Margin="34,38,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160"/>
        <TextBlock Text="Login" HorizontalAlignment="Left" Height="33" Margin="237,38,0,0" VerticalAlignment="Top" Width="129" />

        <TextBox Name="txtBox_password" HorizontalAlignment="Left" Height="27" Margin="34,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160" />
        <TextBlock Text="Password" HorizontalAlignment="Left" Height="33" Margin="237,110,0,0" VerticalAlignment="Top" Width="129" />

        <Button Content="SIGN-IN" HorizontalAlignment="Left" Height="32" Margin="34,180,0,0" VerticalAlignment="Top" Width="251" Click="Button_Signin"/>

        <Button Content="QUITTER" HorizontalAlignment="Left" Height="32" Margin="34,233,0,0" VerticalAlignment="Top" Width="251" Click="Button_Quit_Signin"/>

    </Grid>
</Window>
